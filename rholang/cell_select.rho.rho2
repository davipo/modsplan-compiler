#// cell_select.rho
#// rchain-architecture.readthedocs.io/en/latest/contracts/contract-design.html#rholang-a-concurrent-language

contract Cell.rcv(get, set, state):
    select:
        case get.rcv(rtn), state.rcv(v):
            rtn.send(v)
            state.send(v)
            Cell.send(get, set, state)
        case set.rcv(newValue), state.rcv(v):
            state.send(newValue)
            Cell.send(get, set, state)
