;# squares.L0
declare i32 printf()
func void squares(first, last) {
    var i32 i
    load first
    store i
    br while8
while8:
    load i
    load last
    cmp le
    br whiletrue8, whileend8
whiletrue8:
    ;(continue to while8, break to whileend8)
    const "%2d %4d"
    load i
    load i
    load i
    mul
    call printf 3
    pop
    load i
    const 1
    add
    store i
    br while8
whileend8:
    ret
}
