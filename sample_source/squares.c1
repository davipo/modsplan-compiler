/* squares.c1 */

extern int printf();

void squares() {
    for (int i = 0; i <= 20; i++) {
        printf("%2d  %3d", i, i * i);
    }

    int i = 1;
    while (i <= 20) {
        printf("%2d  %3d", i, i * i);
        i = i + 1;
    }

    int i;
    for (i = 0; i <= 25; i = i + 1) {
        printf("%2d  %3d", i, i * i);
    }

}
